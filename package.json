{
  "name": "@kkna/workspace",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.10.0",
  "repository": "github:importantimport/kkna",
  "scripts": {
    "automd": "automd && corepack pnpm -r exec automd",
    "build": "corepack pnpm -rF ./packages/* build",
    "docs:dev": "corepack pnpm -rF @kkna/docs dev",
    "docs:build": "corepack pnpm build && corepack pnpm -rF @kkna/docs build",
    "docs:preview": "corepack pnpm -rF @kkna/docs preview"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.3",
    "@importantimport/tsconfig": "^0.0.1",
    "automd": "^0.3.7",
    "eslint": "^8.57.0",
    "eslint-plugin-perfectionist": "^2.7.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.10",
    "vitepress": "^1.1.4",
    "vitest": "^1.4.0"
  }
}
